{
    "version": 2,
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/stock",
            "destination": "/api/stock.js"
        },
        {
            "source": "/api/factory-prices",
            "destination": "/api/factory-prices.js"
        },
        {
            "source": "/api/sheet-data",
            "destination": "/api/sheet-data.js"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ]
}